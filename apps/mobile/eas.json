{
    "cli": {
        "version": ">= 3.13.3"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {
            "ios": {
                "appleId": "your.apple.id@example.com",
                "ascAppId": "1234567890",
                "appleTeamId": "ABCD1234"
            },
            "android": {
                "serviceAccountKeyPath": "./path-to-key.json",
                "track": "production"
            }
        }
    }
}